// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Game{
    id String @id @default(uuid())
    group Int
    pricesAlba Int[]
    pricesBatia Int[]
    profitsAlba Int[]
    profitsBatia Int[]
    createdAt DateTime @default(now())
}
